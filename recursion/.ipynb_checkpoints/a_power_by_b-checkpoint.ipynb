   "source": [
    "# Write a Python program to calculate the value of 'a' to the power 'b'."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def power(a,b):\n",
    "    if(b==0):\n",
    "        return 1\n",
    "    elif(b==1):\n",
    "        return a\n",
    "    elif (a==0):\n",
    "        return 0\n",
    "    else:\n",
    "        return a*power(a,b-1)"
   ]
  },
